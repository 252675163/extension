{
	"name": "mrextend",
	"displayName": "MRextend",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:gitlab"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "jwx.gitlab.refresh",
			"title": "提交MR",
			"icon": {
				"dark": "resources/dark/icon-refresh.svg",
				"light": "resources/light/icon-refresh.svg"
			}
		}],
		"viewsContainers": {
            "activitybar": [
                {
                    "id": "jwx",
                    "title": "jwx extend",
                    "icon": "resources/jwx.svg"
                }
            ]
        },
        "views": {
            "jwx": [
                {
                    "id": "gitlab",
                    "name": "gitlab"
                }
            ]
		},
		"menus": {
            "view/title": [
                {
                    "command": "jwx.gitlab.refresh",
                    "when": "view == gitlab",
                    "group": "navigation"
                }
            ]
        }
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
